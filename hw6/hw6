with open("text.txt", "a", encoding="utf-8") as f:
    print("Пожалуйста, вводите латинские слова.")
    a=input()
    if a=="":
        print("Зачем же вы ничего не ввели? :(")
    else:
        while a!="":
            if a.endswith("tur"):
                f.write(a)
                f.write("\n")
            a=input()
            
